@model DatabaseAccess.TblUserType

@{
    ViewBag.Title = "User Type Details";
}

<h4>@ViewBag.Title</h4>
<hr />

<div>

    <dl class="dl-horizontal">
        <dt>
            User Type :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserTypeName)
        </dd>

        <dt>
            Description :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserTypeDescription)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.UserTypeId }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
</p>
