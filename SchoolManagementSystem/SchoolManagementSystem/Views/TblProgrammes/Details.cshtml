@model DatabaseAccess.TblProgramme

@{
    ViewBag.Title = "Programme Details";
}

<h4>@ViewBag.Title</h4>
<hr />

<div>

    <dl class="dl-horizontal">
        <dt>
            Programme Name :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgrammeName)
        </dd>

        <dt>
            Start Date :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgrammeStartDate)
        </dd>

        <dt>
            Status :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgrammeStatus)
        </dd>

        <dt>
            User Name :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TblUser.UserName)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.ProgrammeId }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
</p>
